                     <h2>Artist Wagon</h2>
                  </div>
                  <div class="brand-logo-collapsed">
                     <h2>AR</h2>
                  </div>
               </a>
            </div>
            <!-- END navbar header-->
            <!-- START Nav wrapper-->
            <div class="nav-wrapper">
            	<ul class="nav navbar-nav">
                  <!-- START User avatar toggle-->
                  <li>
                     <a href="${pageContext.request.contextPath}/app" title="Dashboard">Dashboard</a>
                  </li>
                  <li>
                     <a href="${pageContext.request.contextPath}/app/groups" title="All Groups">All Groups</a>
                  </li>
               </ul>
              <!-- START Right Navbar-->
              <ul class="nav navbar-nav navbar-right">
                  <!-- START Alert menu-->
                  <li class="dropdown dropdown-list">
                     <a href="#" data-toggle="dropdown">

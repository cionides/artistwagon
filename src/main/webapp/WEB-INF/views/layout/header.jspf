                                    <div class="pull-left">
                                       <em class="fa fa-sign-out fa-2x"></em>
                                    </div>
                                    <div class="media-box-body clearfix">
                                       <p class="m0 pt-sm pl-lg">Logout</p>
                                    </div>
                                 </div>
                              </a>
                           </div>
                           <!-- END list group-->
